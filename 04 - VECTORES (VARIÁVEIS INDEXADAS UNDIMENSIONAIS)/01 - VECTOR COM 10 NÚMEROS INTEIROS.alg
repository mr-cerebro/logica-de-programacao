/*
 * Faça um algoritmo que preenche um
 * vector NUM com 10 números inteiros
 * e os imprima.
 */

 inicio
 	inteiro NUM[10], i
 	para i de 0 ate 9
 		escreva "digite o ", i + 1, "º número: "
 		leia NUM[i]
 	fimpara
 	escreva "\n"
 	para i de 0 ate 9
 		escreva "[", NUM[i], "]\n"
 	fimpara
fimalgoritmo	
 		