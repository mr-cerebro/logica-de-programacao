inicio
	inteiro A, AUX, B
	escreva "Digite o valor de A: "
	leia A
	escreva "Digite o valor de B: "
	leia B
	escreva "\n"
	escreva "O valor digitado de A foi: ", A, "\n"
	escreva "O valor digitado de B foi: ", B, "\n\n"
	AUX <- A
	A <- B
	B <- AUX
	escreva "O valor trocado de A é: ", A, "\n"
	escreva "O valor trocado de B é: ", B
fimalgoritmo